for i in $(grep _2 List); do echo "--"$i >> file_DE; for moea in $(ls data-instances/ | grep DE);  do echo data-instances/${moea}/${i}>>file_DE ;  done; done
for i in $(grep _3 List); do echo "--"$i >> file_DE; for moea in $(ls data-instances/ | grep DE);  do echo data-instances/${moea}/${i}>>file_DE ;  done; done
